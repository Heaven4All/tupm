[TUPAccount]

TUPUser=			# Fill with tup login
TUPPass=			# Fill with your password
TUPEncryptedPass=0		# 0 = Clear password, 1 = Encrypted password
TUPMachine=			# Fill with a name for your computer

TUPUptimeInterval=30		# Uptime sent interval (only in daemon mode)
TUPFlushLogAtStart=1		# Flush Logs when TUPm start (log and status)

[TUPServer]

TUPHostName=update.uptimeprj.com

[TUPMonitor]

TUPMonitorEngine=0		# 1 to enable Monitor Engine, 0 to disable
TUPMonitorInterval=5		# Monitor check interval (only in daemon mode)

[TUPNetwork]

TUPInterface=eth0		# fill with net interface (eth0,rl0,xl0,...)

[Proxy]

UseProxy=0			# 1 to enable Proxy, 0 to disable
ProxyHost=10.1.0.1
ProxyPort=3128



